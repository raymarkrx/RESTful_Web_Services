<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://java.sun.com/xml/ns/javaee" 
xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" 
xmlns:web="http://java.sun.com/xml/ns/javaee" 
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
id="WebApp_ID" version="2.5">
  <display-name>dw.spring3.rest</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>
			/WEB-INF/rest-context.xml
		</param-value>
  </context-param>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>rest</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>rest</servlet-name>
    <url-pattern>/service/*</url-pattern>
  </servlet-mapping>
  <jsp-config>
    <taglib>
      <taglib-uri>http://java.sun.com/jsp/jstl/core</taglib-uri>
      <taglib-location>/WEB-INF/tags/c.tld</taglib-location>
    </taglib>
  </jsp-config>
  <servlet>
    <servlet-name>jsonEventServlet</servlet-name>
    <servlet-class>com.aichuche.servlet.JsonEventServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>jsonEventServlet</servlet-name>
    <url-pattern>/json</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>reportdataServlet</servlet-name>
    <servlet-class>com.aichuche.servlet.chh.ReportDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>reportdataServlet</servlet-name>
    <url-pattern>/reportdata</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>connectChangedServlet</servlet-name>
    <servlet-class>com.aichuche.servlet.chh.ConnectChangedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>connectChangedServlet</servlet-name>
    <url-pattern>/connect_changed</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>downpushServlet</servlet-name>
    <servlet-class>com.aichuche.servlet.chh.DownPushServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>downpushServlet</servlet-name>
    <url-pattern>/downpushServlet</url-pattern>
  </servlet-mapping>

  
  <servlet>
    <servlet-name>testServlet</servlet-name>
    <servlet-class>com.aichuche.servlet.chh.TestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>testServlet</servlet-name>
    <url-pattern>/testServlet</url-pattern>
  </servlet-mapping>
</web-app>